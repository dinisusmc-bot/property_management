          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT c.id, c.trip_date, c.status, c.passengers, CAST(SPLIT_PART(c.last_checkin_location, ',', 1) AS FLOAT) AS latitude, CAST(SPLIT_PART(c.last_checkin_location, ',', 2) AS FLOAT) AS longitude, c.last_checkin_time, v.name AS vehicle_name, cl.name AS client_name, d.full_name AS driver_name FROM charters c LEFT JOIN vehicles v ON c.vehicle_id = v.id LEFT JOIN clients cl ON c.client_id = cl.id LEFT JOIN users d ON c.driver_id = d.id WHERE c.last_checkin_location IS NOT NULL AND c.last_checkin_location != '' AND c.status IN ('booked', 'in_progress') ORDER BY c.last_checkin_time DESC",
