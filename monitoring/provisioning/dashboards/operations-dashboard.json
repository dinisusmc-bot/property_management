          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  c.id as \"Charter ID\",\n  c.trip_date as \"Trip Date\",\n  v.name as \"Vehicle\",\n  CASE WHEN c.vendor_id IS NULL THEN 'No' ELSE 'Yes' END as \"Vendor Assigned\",\n  CASE WHEN c.driver_id IS NULL THEN 'No' ELSE 'Yes' END as \"Driver Assigned\",\n  c.passengers as \"Passengers\"\nFROM charters c\nJOIN vehicles v ON c.vehicle_id = v.id\nWHERE (c.vendor_id IS NULL OR c.driver_id IS NULL)\n  AND c.status != 'cancelled'\nORDER BY c.trip_date;",
          "refId": "A"
        }
